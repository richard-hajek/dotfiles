echo "Sourcing ~/.zshrc"

autoload -Uz compinit
compinit -d ~/.cache/zsh/zcompdump-$ZSH_VERSION

source ~/.config/shellrc
source ~/.config/zshrc_ohmyzsh

setopt extendedglob
setopt COMPLETE_ALIASES
setopt no_share_history
unsetopt share_history

[ -f /usr/share/nvm/init-nvm.sh ] && source /usr/share/nvm/init-nvm.sh

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/home/meowxiik/.local/miniconda3/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/meowxiik/.local/miniconda3/etc/profile.d/conda.sh" ]; then
        . "/home/meowxiik/.local/miniconda3/etc/profile.d/conda.sh"
    else
        export PATH="/home/meowxiik/.local/miniconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

